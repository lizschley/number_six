  "paragraphs": [
    {
      "id": "first",
      "subtitle": "Find the string in tuple from a substring",
      "note": "",
      "text": [
                "<p>Find the Janes:</p>",
                "<pre>",
                "test_tuple =('Jane Austen', 'Anne Sullivan', 'Ada Lovelace', 'Jane Goodall', 'Marie Curie')",
                "substr= 'Jane'",
                "janes = [i for i in test_tuple if substr  in i]",
                "print(janes)",
                "</pre>",
                "<p>Gives you: ['Jane Austen', 'Jane Goodall']</p>"
              ]
    },
    {
      "id": "second",
      "subtitle": "Comma delimited list of ids from column to use in other queries",
      "note": "<p><strong>* Note</strong> - MySQL code was from my notes, references lost</p>",
      "text": [
                "<p style='text-decoration: underline;'>MySQL</p>",
                "<pre>",
                "select group_concat(distinct ac.animal_id, SEPARATOR ',')",
                "from animal_caretaker ac",
                "where ac.caretaker_id = 15",
                "</pre>",
                "<p style='text-decoration: underline;'>Postgres</p>",
                "<pre>",
                "select string_agg(distinct cast (ac.animal_id as varchar), ',') as animal_ids",
                "from animal_caretaker ac",
                "where ac.caretaker_id = 15",
                "</pre>"
      ]
    }
  ],
